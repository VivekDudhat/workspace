<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data>
    
          <record id="hotel_booking_report_action" model="ir.actions.report">
            <field name="name">Booking Details</field>
            <field name="model">hotel.room.booking</field>
            <field name="report_type">qweb-pdf</field>
            <field name="report_name">hotel_management.report_hotel_booking_template</field>
            <field name="report_file">hotel_management.report_hotel_booking_template</field>
            <field name="binding_model_id" eval="False"/>
            <field name="binding_type">report</field>
            <field name="print_report_name">'Booking Detail - %s' % (object.customer_name or "Unknown Patient")</field>
        </record>
    
    </data>
    
</odoo>
